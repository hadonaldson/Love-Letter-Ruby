<p id="notice"><%= notice %></p>

<h1>Contact Us</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Wedding date</th>
      <th>Getting ready location</th>
      <th>Service type</th>
      <th>Number of services</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consults.each do |consult| %>
      <tr>
        <td><%= consult.Name %></td>
        <td><%= consult.Wedding_Date %></td>
        <td><%= consult.Getting_Ready_Location %></td>
        <td><%= consult.Service_Type %></td>
        <td><%= consult.Number_of_Services %></td>
        <td><%= consult.Comments %></td>
        <td><%= link_to 'Show', consult %></td>
        <td><%= link_to 'Edit', edit_consult_path(consult) %></td>
        <td><%= link_to 'Destroy', consult, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consult', new_consult_path %>
